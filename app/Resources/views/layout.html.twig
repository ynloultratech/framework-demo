    {#<script src="{{ path('ynlo_framework_js') }}"></script>#}
{% endblock %}

{% block headline %}{% block title %}{% endblock %}{% endblock headline %}

 {% block footer %}
     <p>&copy; <a href="https://www.ynloultratech.com" target="_blank">YnloUltratech</a> {{ 'now'|date('Y') }}</p>
 {% endblock footer %}

{% block navbar %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                        aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('home') }}"><i class="fa fa-rocket"></i> YnloFramework</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                {{ mopa_bootstrap_menu('AppBundle:Builder:mainMenu',{'automenu': 'navbar', 'autochilds':true}) }}
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </nav>
{% endblock %}

{% block foot_script %}
    <script type="text/javascript">
        require(['bootstrap'], function () {
            $(document).ready(function () {
                $('[data-toggle="tooltip"]').tooltip();
                $('[data-toggle="popover"]').popover();
            });
        });
    </script>
{% endblock %}